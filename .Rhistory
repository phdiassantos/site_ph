aacpd <- function(Severidade,tempo){
#n é o tamanho do valor criado como "tempo", ou
# 0 total do número de datas amostradas
n <- length(tempo)
#meanvec é o vetor (matriz com uma dimensão)
#que contém a média (%) da infecção
#iincializa com -1 para todas as entradas
meanvec <- matrix(-1,(n-1))
#intvec é o vetor que contém o tamanho do tempo entre as datas de amostragem
intvec <- matrix(-1,(n-1))
#o loop  (repetir o codigo para todas as variáveis) vai do primeiro para a penúltima entrada
#a chave esquerda indica o início de  comandos no loop
for(i in 1:(n-1)){
# a entrada i na fuanção é substituído pela média da % de infecção
# entre o tempo da amostra i e o tempo da amostra i + 1
meanvec[i] <- mean(c(Severidade[i],
Severidade[i+1]))
#a entrada no intvec é substituída pelo comprimento
# do intervalo de tempo entre o tempo i e o tempo i + 1
intvec[i] <- tempo[i+1] - tempo[i]
#a chave direita termina o loop
}
#os dois vetores são multiplicados juntos, uma entrada de cada vez
infprod <- meanvec * intvec
#a soma das entradas no vetor resultante
#   devolve AUDPC
sum(infprod)
#o colchete direito encerra a função
}
#Agora, aplique a função aos dados do exemplo e coloque
#o resultado em um novo objeto chamado 'AACPDexemplo'
aacpd(Severidade,tempo) -> AACPDexemplo
#Mostra o Valor da AACPD
AACPDexemplo
#Desenhar um retangulo em volta do valor
rect(0.1,(sev3+.3),2,(sev3+1),border="black")
#AUDPC Text
text(1.05,(sev3+0.8),"AUDPC")
text(1.05,(sev3+0.5),AACPDexemplo)
