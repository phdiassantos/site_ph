## Por conveniencia, os dados devem ser formatados em um dataframe
severidade2 <- as.data.frame(cbind(sevdoenca,temp))
severidade.lm <- lm(sevdoenca~temp,data=severidade2)
## Gerar um sumario do modelo linear
summary(severidade.lm)
## which=1 produz um gráfico de valores residuais x valores ajustados
plot(severidade.lm, which=1)
## which = 2 produz um gráfico de um gráfico de quantil-quantil (QQ)
plot(severidade.lm,which=2)
options(digits=4)
fit.with.se<-predict(
severidade.lm,
se.fit=TRUE
)
data.frame(
severidade2,
fitted.value=predict(severidade.lm),
residual=resid(severidade.lm),
fit.with.se
)
plot(
sevdoenca~temp,
data=severidade2,
xlab="Temperatura",
ylab="Severidade de doen?a (%)",
pch=16
)
