plotexp <- function(y0,r,maxt){
curve(
y0*exp(r*x),
from=0,
to=maxt,
xlab='Tempo',
ylab='Incidência de doença',
col='mediumblue')
}
plotexp(0.0017, 0.01579, 100)
plotmono <- function(y0,r,maxt){
curve(
1-(1-y0)*exp(-r*x),
from=0,
to=maxt,
xlab='Tempo',
ylab='Incidência de doença',
col='mediumblue')
}
plotmono(0.0017, 0.00242, 2000)
plotlog <- function(y0,r,maxt){
curve(
1/(1+(1-y0)/y0*exp(-r*x)),
from=0,
to=maxt,
xlab='Tempo',
ylab='Incidência de doença',
col='mediumblue'
)
}
plotlog(0.001, 0.01636, 1000)
plotgomp <- function(y0,r,maxt){
curve(
exp(log(y0)*exp(-r*x)),
from=0, to=maxt, xlab='Tempo',
ylab='Incidência de doença',
col='mediumblue'
)
}
plotgomp(0.0017,0.02922, 250)
plotweib <- function(a,b,c,maxt){
curve(
1-exp(-((x-a)/b)^c),
from=0,
to=maxt,
xlab='Tempo',
ylab='Incidência de doença',
col='mediumblue'
)
}
plotweib(1, 331.10, 10.04, 500)
